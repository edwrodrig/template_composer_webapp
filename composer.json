{
    "name": "tpl_company_tpl/tpl_project_tpl",
    "description" : "Una asombrosa aplicación web",
    "authors": [
        {
            "name": "Edwin Rodríguez-León",
            "homepage": "http://www.edwin.cl",
            "role": "Developer"
        }
    ],
    "minimum-stability" : "stable",
    "require": {
        "php" : "^7.4",
        "labo86/staty" : "0.3.0",
        "labo86/hapi" : "^0.1",
        "edwrodrig/image" : "^3.0",
        "ext-json": "*",
        "ext-imagick": "*"
    },
    "require-dev" : {
        "phpunit/phpunit" : "^8.5"
    },
    "autoload" : {
        "psr-4" : {
            "tpl_company_tpl\\tpl_project_tpl\\model\\" : "modules/model/src/",
            "tpl_company_tpl\\tpl_project_tpl\\site\\" : "modules/site/src/",
            "tpl_company_tpl\\tpl_project_tpl\\controller\\" : "modules/controller/src/"
        }
    },
    "autoload-dev" : {
        "psr-4" : {
            "test\\tpl_company_tpl\\tpl_project_tpl\\model\\" : "modules/model/test/",
            "test\\tpl_company_tpl\\tpl_project_tpl\\site\\" : "modules/site/test/",
            "test\\tpl_company_tpl\\tpl_project_tpl\\controller\\" : "modules/controller/test/"
        }
    }
}
