{
    "name": "tpl_company_tpl/tpl_project_tpl",
    "description" : "Una asombrosa aplicación web",
    "authors": [
        {
            "name": "Edwin Rodríguez-León",
            "homepage": "http://www.edwin.cl",
            "role": "Developer"
        }
    ],
    "minimum-stability" : "stable",
    "require": {
        "php" : "^7.4",
        "labo86/staty" : "^0.3",
        "labo86/hapi" : "^2.0",
        "labo86/rdtas" : "^4.2",
        "edwrodrig/image" : "^3.4",
        "ext-json": "*",
        "ext-imagick": "*",
        "ext-pdo": "*",
        "ext-fileinfo": "*"
    },
    "require-dev" : {
        "phpunit/phpunit" : "^8.5"
    },
    "autoload" : {
        "psr-4" : {
            "tpl_company_tpl\\tpl_project_tpl\\app\\" : "modules/app/src/",
            "tpl_company_tpl\\tpl_project_tpl\\site\\" : "modules/site/src/"
        }
    },
    "autoload-dev" : {
        "psr-4" : {
            "test\\tpl_company_tpl\\tpl_project_tpl\\app\\" : "modules/app/test/",
            "test\\tpl_company_tpl\\tpl_project_tpl\\site\\" : "modules/site/test/"
        }
    }
}
